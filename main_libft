#include "libft.h"

int main()
{
	/* _____FT_MEMSET_____ */
	char c[] = "1312";
	printf("%s\n", (char*)ft_memset(c, 67, 5));
	char c1[] = "1312";
	printf("%s", (char*)memset(c1, 66, 5));
}

	/* _____FT_STRLEN_____ */

//	char *i;
//	i = "123456789012123";

//	printf("%s\n", ft_strlen(i));

	/* _____FT_MEMCPY_____ */

    

	

	/*   _____FT_MEMCCPY_____  */

//	void *dest;
