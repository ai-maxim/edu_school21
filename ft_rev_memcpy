#include "libft.h" 

void *ft_rev_memcpy(void *str)
{
    unsigned char *s;
    size_t len;
    size_t i;
    size_t temp;

    len = 0;
    s = (unsigned char *)str;
    while (s[len])
        len++;
    i = 0;
    while (i < len / 2)
    {
        temp = s[i];
        s[i] = s[len - 1 - i];
        s[len - 1 - i] = temp;
        i++;
    }
    return (s);
}
int main()
{
    char s1[] = "Hello";

    printf("%s\n", ft_rev_memcpy(s1));
}